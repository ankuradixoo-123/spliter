generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int            @id @default(autoincrement())
  email    String         @unique
  name     String?
  password String?
 // groups   GroupMember[]
//  createdGroups Group[]
//  expenses Expense[]      @relation("UserExpenses")
}